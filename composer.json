{
    "name": "qorbarsi/shop-skeleton",
    "description": "Skeleton for e-comerce MVP develop",
    "keywords": ["yii2", "yii", "skeleton", "framework", "advanced", "project template", "shop", "dvizh"],
    "homepage": "https://github.com/qorbarsi",
    "type": "project",
    "license": "apache",
    "authors": [
      {
        "name": "Qorbarsi",
        "email": "qorbarsi@gmail.com"
      }
    ],
    "support": {
        "issues": "https://github.com/qorbarsi/"
    },
    "minimum-stability": "dev",
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/qorbarsi/yii2-shop"
        },
        {
            "type": "vcs",
            "url": "https://github.com/qorbarsi/yii2-cart"
        },
        {
            "type": "vcs",
            "url": "https://github.com/qorbarsi/yii2-promocode"
        },
        {
            "type": "vcs",
            "url": "https://github.com/qorbarsi/yii2-order"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-authclient": "~2.1.0",
        "dvizh/yii2-shop": "dev-dev-custom",
        "dvizh/yii2-order": "dev-custom",
        "dvizh/yii2-cart": "dev-custom",
        "dvizh/yii2-promocode": "dev-custom",
        "dvizh/yii2-review": "@dev",
        "dvizh/yii2-certificate": "@dev",
        "dmstr/yii2-adminlte-asset": "2.*",
        "pheme/yii2-settings": "*",
        "2amigos/yii2-editable-widget": "~1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
    	"psr-4": {
    	    "dektrium\\user\\": "common/modules/dektrium/yii2-user",
    	    "dektrium\\rbac\\": "common/modules/dektrium/yii2-rbac"
    	}
    }
}
